<%= stylesheet_link_tag "corporation" %>
<%= javascript_include_tag "forme_juridique", type: :module %>

<html>
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
</head>
<div class="AIO">
  <%= form_with(model: [@corporation, @adherent, @formulaire], url: corporation_adherent_formulaire_path(@corporation, @adherent, @formulaire), method: :patch) do |form| %>
    <div class="haut">
      <div class="images">
        <div class="img-gauche" >
          <img src="data:image/png;base64," alt="Grapefruit slice atop a pile of other slices" />
        </div>
        <div class="img-droite" >
          <img class="img-img-droite" src="data:image/png;base64," alt="Grapefruit slice atop a pile of other slices" />
        </div>
      </div>
      <h1> <%=@donnees.nom_formulaire %> <%= @formulaire.annee %> </h1>
    </div>

    <!---------------------------------- PART 1 --------------------------------------------->

    <div class="corp">
      <div class="part1">
        <div class="l">
          <div class="sixp">
            <p> A compléter et à retourner à :</p>
          </div>
          <div class="quatrep">
            <%= form.label :raison_sociale, "Raison Sociale" %>
            <%= form.text_field :raison_sociale, readonly: @show_mode, disabled: @show_mode, class: "appearance-none", class: 'custom-text-field'%>
          </div>
        </div>

        <div class="l">
          <div class="sixp">
            <p><%= @donnees.nom_formulaire %></p>
          </div>
          <div class="quatrep">
            <%= form.label :nom_prenom, "Nom Prénom" %>
            <%= form.text_field :nom_prenom, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
          </div>
        </div>

        <div class="l">
          <div class="sixp">
            <p> <%= @donnees.adresse%></p>
          </div>
          <div class="quatrep">
            <%= form.label :adresse, "Adresse" %>
            <%= form.text_field :adresse, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
          </div>
        </div>

        <div class="l">
          <div class="sixp">
            <p></p>
          </div>
          <div class="quatrep">
            <%= form.label :cp_ville, "CP Ville" %>
            <%= form.text_field :cp_ville, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
          </div>
        </div>
      </div>
    </div>


    <!---------------------------------- PART 2 --------------------------------------------->

    <div class="part2">
      <h2 class="copama">Vous </h2>
      <div class="field">
        <div class="sixp sixp2">
          <%= form.label :telephone, "Numéro téléphone :" %>
          <%= form.telephone_field :telephone, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
        <div class="quatrep">
          <%= form.label :portable, "Numéro portable :" %>
          <%= form.telephone_field :portable, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
      </div>

      <div class="field">
        <div class="sixp sixp2">
          <%= form.label :email, "E-mail : " %>
          <%= form.email_field :email, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
        <div class="quatrep"></div>
      </div>

      <div class="field">
        <%= form.label :statut, "Statut du chef d'entreprise * :"%>
        <%= form.radio_button :statut, 'Non-salarié', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode, class:"statut" %> Non-salarié
        <%= form.radio_button :statut, 'Salarié', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode, class:"statut" %> Salarié
      </div>
    </div>

    <!---------------------------------- PART 3 --------------------------------------------->

    <div class="part3">
      <h2 class="copama"> Votre entreprise</h2>
      <div class="form-options">
        <%= form.label :forme_juridique, "Forme juridique :" %>
        <%= form.radio_button :forme_juridique, 'Ent. ind.', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> Ent. ind.<br>
        <%= form.radio_button :forme_juridique, 'EIRL', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> EIRL<br>
        <%= form.radio_button :forme_juridique, 'EURL', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> EURL<br>
        <%= form.radio_button :forme_juridique, 'SARL', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> SARL<br>
        <%= form.radio_button :forme_juridique, 'SAS', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> SAS<br>
        <%= form.radio_button :forme_juridique, 'SASU', class: 'form-check-input', readonly: @show_mode, disabled: @show_mode %> SASU<br>
        <%= form.radio_button :forme_juridique, 'Autre', class: 'form-check-input', id: 'autre-option', readonly: @show_mode, disabled: @show_mode %> Autre :
        <%= form.text_field :forme_juridique, { class: 'text-field custom-text-field autre_option-text', id:'autre_option-text', disabled: true }%>

      </div>



      <div class="field">
        <div class="sixp sixp2">
          <%= form.label :code_naf_ape, "Code NAF / APE : " %>
          <%= form.text_field :code_naf_ape, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
        <div class="quatrep">
          <%= form.label :siret, " SIRET : " %>
          <%= form.text_field :siret, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
      </div>

      <div class="field">
        <div class="sixp sixp2">
          <%= form.label :nb_salaries, "Nombre de salariés * : " %>
          <%= form.text_field :nb_salaries, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
        <div class="quatrep">
          <%= form.label :nb_apprentis, "dont  " %>
          <%= form.text_field :nb_apprentis, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>apprentis
        </div>
      </div>
      <div class="field">
        <p class="petit"> *Ces informations sont uniquement utilisées pour le calcul de la représentativité du métier</p>
      </div>
    </div>

    <!---------------------------------- PART 4 --------------------------------------------->

    <div class="part part4">
      <h2> Votre cotisation </h2>

      <div class="field">
        <script type="module" src="/assets/estheticienne.js"></script>
        <%= form.label :cotisation, "Salon sans salarié ou Coiffure à domicile " %>
        <%= form.check_box :checkbox_2, class: 'amount-checkbox', readonly: @show_mode, disabled: @show_mode, data: { amount: @donnees.montant_parametre_1 } %>
        <%= "#{ @donnees.montant_parametre_1 }€" %><br>

        <%= form.label :cotisation, "Salon jusqu'à 4 salariés inclus " %>
        <%= form.check_box :checkbox_3, class: 'amount-checkbox', readonly: @show_mode, disabled: @show_mode, data: { amount: @donnees.montant_parametre_2 } %>
        <%= "#{ @donnees.montant_parametre_2 }€" %><br>

        <%= form.label :cotisation, "Cotisation par Salon supplémentaire " %>
        <%= form.number_field :champ_1, class: 'form-control', id: 'amount-to-multiply', readonly: @show_mode, disabled: @show_mode, data: { multiply_amount: @donnees.montant_parametre_3 } %>
        <%= "#{ @donnees.montant_parametre_3 }€"%>
        <%= form.text_field :champ_2, readonly: true, id: 'Multiply-amount-field', readonly: @show_mode, disabled: @show_mode %>
        <br>

        <%= label_tag 'total_amount', 'Total à payer : ' %>
        <%= form.text_field :champ_3 , readonly: true, id: 'total-amount-field', readonly: @show_mode, disabled: @show_mode %>
      </div>
    </div>

    <!---------------------------------- PART 5 --------------------------------------------->



    <div class="part5">
      <h2 class="copama"> Mode paiement </h2>


      <div class="cotisation">
        <div class="sous-cotisation">
          <%= form.radio_button :mode_paiement, 'prelevement_automatique_mensuel', class: 'form-check-input coti-rb', readonly: @show_mode, disabled: @show_mode %> <p> <b> Prélèvement automatique mensuel </b> (mandat au verso à compléter, signer et à retourner avec votre RIB**) </p>
        </div>
        <p class="etoile"> ** Prélèvement effectué de janvier à novembre soit sur 11 mois </p>
        <br>
        <div class="sous-cotisation">
          <%= form.radio_button :mode_paiement, 'cheque', class: 'form-check-input coti-rb', readonly: @show_mode, disabled: @show_mode %> <p> <b>Chèque</b> à l'odre de la <%= @donnees.nom_formulaire %> </p>
        </div>
        <br>
        <div class="sous-cotisation">
          <%= form.radio_button :mode_paiement, 'virement_bancaire', class: 'form-check-input coti-rb', readonly: @show_mode, disabled: @show_mode %> <p> <b> Virement bancaire</b> <%= @donnees.banque %> </p>
        </div>
        <b class="iban"> IBAN : <%= @donnees.iban %> - BIC : <%= @donnees.bic %> </b>
      </div>

      <div class="cadre-coti boulanger">
        <h3 class="h3-coti"> Option assurance protection juridique (voir documents joints)</h3>
        <p><b>Pour souscrire à la protection juridique, vous trouverez un bulletin d'adhésion en pièce jointe de ce bulletin. </b> Pour une vue synthétique des garanties offertes par l'assurance protection juridique, référez-vous à la brochure jointe. </p>
      </div>

      <p class="petit2"> L'adhésion est valable pour l'année civile et se renouvelle automatiquement par tacite reconduction à l'échéance. </p>
      <b class="centrer">Je déclare m'affilier à la <%= @donnees.nom_formulaire %> et je bénéficie ainsi de l'adhésion combinée à la CAPEB du Haut-Rhin. </b>

      <div class="field l">
        <div class="sixp">
          <%= form.label :lieu, "Fait à : " %>
          <%= form.text_field :lieu, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>

          <%= form.label :date, "Date :" %>
          <%= form.date_field :date, class: 'date-input', pattern: '\d{2}/\d{2}/\d{4}', placeholder: 'Format : jj/mm/aaaa', readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
        <div class="quatrep"></div>
      </div>

      <p class="signature"> Signature : </p>
      <div class="verso">
        <h3> Verso à Compléter </h3> <img src="<%= asset_path('664866.png') %>" width="70" height="70" alt="fleche">
      </div>

    </div>

    <div class="page-break"></div>

    <div class="part6 boulanger">
      <h3 class="p2-h3"> Politique de confidentialié du traitement des données personnelles : </h3>
      <br>
      <p>Les informations recueillies sont nécessaires por permettre à la corporation de taiter et gérer votre adhésion, vos abonnements, l'envoi de lettres d'information sur les acions menées mais aussi, votre accès aux différents produits et services qu'elle met à votre disposition. Pour ce faire, la corporation dispose de moyens informatiques. Dans ce cadre, vos données personelles sont susceptibles d'être transmises à des sous-traitants ou prestataires.</p>
      <br>
      <p> Ces informations et données personnelles sont également conservées à des fins de gestion des organisations professionnelles, afin de respecter les obligations légales et réglementaires. Elles seront conservées aussi longtemps que nécessaire pour la gestion des oranisations. </p>
      <br>
      <p>Conformément aux dispostions de la loi "Informatique et Libertés" du 6 janvier 1978 modifiée (UE) n°2016/679, vous disposez d'un droit d'accès, de rectification, du suppresion, de limitation du traitement à certains cas de figure, de retrait de votre consentement et de portabilité des données vous concernant. Vous disposez également d'un droi au testament numérique (traitement de vos données post-mortem).</p>
      <br>
      <p>Si vous souhaitez exercer ces droits, ou vous désabonner de la newsletter, adressez-vous au Reponsable de traitement des données par e-mail à : <b> corpos@artifrance.fr</b></p>
      <br>
      <p> Vous diposez également d'un droit de recours auprès du CNIL en cas de violation de la réglementation applicable en matière de protection des donénes personnelles et notamment du RGPD.</p>
      <br>


      <%= form.check_box :checkbox_1, disabled: @show_mode %>
      <%= form.label :checkbox_1, "Je déclare accepter la transmission de mes données personnelles à des partenaires de la coporation uniquement aux fins de campagnes d'information, d'enquêtes, de publication dans des annuaires professionnels, ou de diffusion de brochures pour bénéficier d'avantages tarifaires. ", class:"cb-bas" %>
    </div>



    <div class="part7 boulanger2">
      <h1> Mandat de Prévèlevement SEPA</h1>

      <p class="psepa"> En signant ce formulaire de mandat, vous autorisez CORPORATIONS UGA à envoyer des instructions à votre banque pour débiter votre compte, et votre banque à débiter votre conformément aux instructions de CORPORATIONS UGA. Vous bénéficiez du droit d'être remboursé par votre banque selon les conditions décrites dans la convention que vous avez passée avec elle. Une demande de remboursement doit être présentée dans les 8 semaines suivant la date de débit de vitre compte pour un prélèvement autorisé. </p>

      <div class="sepa sepa-part">
        <div class="sepa20">
          <%= form.label :titulaire_compte, "Titulaire du compte : " %>
        </div>
        <div class="sepa80">
          <%= form.text_field :titulaire_compte, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa">
        <div class="sepa20">
          <%= form.label :adresse_sepa, "Adresse postale : " %>
        </div>
        <div class="sepa80">
          <%= form.text_field :adresse_sepa, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa">
        <div class="sepa20"><p></p></div>
        <div class="sepa80"><p> Numéro et nom de la rue </p></div>
      </div>

      <div class="sepa">
        <div class="sepa20"> <p></p></div>
        <div class="sepa80">
          <%= form.text_field :cp_ville_sepa, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa  sepa-part">
        <div class="sepa20"><p></p></div>
        <div class="sepa80"><p> Code postal et Ville </p></div>
      </div>

      <div class="sepa">
        <div class="sepa20"><p> Pays : </p></div>
        <div class="sepa80"><b> FRANCE </b></div>
      </div>


      <div class="sepa">
        <div class="sepa20">
          <%= form.label :iban_sepa, "Coordonnées bancaires : " %>
        </div>
        <div class="sepa80">
          <%= form.text_field :iban_sepa, readonly: @show_mode, disabled: @show_mode, class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa  sepa-part">
        <div class="sepa20"><p></p></div>
        <div class="sepa80"><p> Numéro d'identification international du compte bancaire - IBAN ( International Bank Account Number) </p></div>
      </div>

      <div class="sepa">
        <div class="sepa20"><p></p></div>
        <div class="sepa80">
          <%= form.text_field :bic_sepa, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa  sepa-part">
        <div class="sepa20"><p></p></div>
        <div class="sepa80"><p> Code International d'identification de votre compte - BIC (Bank Identifier Code) </p></div>
      </div>

      <div class="ligne-orange"></div>

      <div class="sepa-orange  sepa-part">
        <div class="sepa20"><p>Nom du Créancier :</p></div>
        <div class="sepa80"><p> <%= @donnees.nom_creancier %> </p></div>
      </div>

      <div class="sepa-orange  sepa-part">
        <div class="sepa20"><p> Identifiant du créancier : </p></div>
        <div class="sepa80"><p> <%= @donnees.id_creancier %> </p></div>
      </div>

      <div class="sepa-orange  sepa-part">
        <div class="sepa20"><p> Adresse du créancier : </p></div>
        <div class="sepa80"><p> <%= @donnees.addresse_creancier %> </p></div>
      </div>

      <div class="sepa-orange  sepa-part">
        <div class="sepa20"><p> Pays du créancier :</p></div>
        <div class="sepa80"><p> <%= @donnees.pays_creancier.upcase %> </p></div>
      </div>

      <div class="sepa-orange  sepa-part">
        <div class="sepa20"><p> Type de paiement :</p></div>
        <div class="sepa80"><p> <%= @donnees.type_paiement %> </p></div>
      </div>

      <div class="ligne-orange"></div>

      <div class="sepa">
        <div class="sepa20">
          <%= form.label :lieu_date_sepa, "Fait à : " %>
        </div>
        <div class="sepa80">
          <%= form.text_field :lieu_date_sepa, readonly: @show_mode, disabled: @show_mode,  class: 'custom-text-field' %>
        </div>
      </div>

      <div class="sepa  sepa-part">
        <div class="sepa20"><p></p></div>
        <div class="sepa80"><p> Lieu et date </p></div>
      </div>

      <div class="sepa">
        <div class="sepa20">
          <p> Signature: </p>
        </div>
      </div>
    </div>
    <% unless @show_mode %>
      <%= form.submit "Enregistrer les modifications", class: "bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline disabled:opacity-50 disabled:cursor-not-allowed mt-4" %>
    <% end %>
  <% end %>
</div>
</html>