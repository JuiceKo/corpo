<div class="flex flex-col">
  <%= link_to "Retour", corporation_path(@corporation, selected_year: session[:selected_year]), data: { turbo: false }, class: "bg-blue-500 text-white font-bold py-2 px-4 rounded mb-4" %>

  <% corporation_id = params[:corporation_id] %>
  <% if corporation_id == '1' %>
    <%= render partial: 'peintres' %>
  <% elsif corporation_id == '2'%>
    <%= render partial: 'boulangers' %>
  <% else %>
    <% redirect_to root_path %>
  <% end %>

</div>



